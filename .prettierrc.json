{
  "trailingComma": "none",
  "semi": true,
  "singleQuote": true,
  "tailwindConfig": "./tailwind.config.js",
  "plugins": ["prettier-plugin-tailwindcss"],
  "pluginSearchDirs": false,
  "jsxSingleQuote": true,
  "quoteProps": "as-needed",
  "useTabs": false,
  "tabWidth": 2,
  "arrowParens": "avoid",
  "printWidth": 80,
  "endOfLine": "lf"
}
